<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;"  data-backdrop="static" data-keyboard="false">
   <div class="modal-dialog">
      <!-- Modal content-->
      <div class="modal-content">
         <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title"><b>{{title}}</b> SEO</h4>
         </div>
            <form method='post'>
         <div class="modal-body">
         
               <label for="demo">Category</label>
               <select name="secondid" id="secondid">
                  <option value="" disabled  hidden>Select category</option>
                  {{{subcategory}}}
               </select>

                


               <label>Url(slug)</label>
               <input type="text" id='url' name='url'>
               <label>Title</label>
               <input type="text" name='title' id='seotitle'>
                <label>Keywords</label>
               <input type="text" name='keywords' id='seokeywords'>

               <label>Author</label>
               <input type="text" name='author' id='seoauthor'>
               <label>Description</label>
               <textarea rows="4" id='seodescription' cols="30" placeholder="Description" name='description'></textarea>
            
         </div>
         <div class="modal-footer">
         <input type="hidden" name='seoid' id='seoid'>
         <input type="hidden" name='currentsecondid' value='{{currenturlid}}'>
            <button type="submit" class="btn btn-default" value='submit' name='submit'>Submit</button>
            <button type="button" class="btn btn-cancel" data-dismiss="modal">Close</button>
         </div>
         </form>
      </div>
   </div>
</div>

<table id="example" class="table table-striped table-bordered" style="width:100%">
   <thead>
      <tr>
         <td>Title</td>
         <td>Seo Title</td>
         <td>URl</td>
         <th>Keywords</th>
        <!--  <th>Description</th> -->
        
         <th>Action</th>
      </tr>
   </thead>
   <tbody>
       {{#filterdata}}

      <tr>
         <td>{{title}}</td>
         <td>{{seotitle}}</td>
         <td>{{seoslug}}</td>
         <td>{{seokeywords}}</td>
        <!--  <td>{{description}}</td> -->
        
         <td>
            <ul class="mmenu">
               <li><a href="javascript:void(0)"  class='edit editmodal' dataid="{{id}}" datathid="{{th_id}}" datascid="{{currentsecondid}}" ><i class="fas fa-pen"></i></a></li>
               {{#seoid}}
               <li><a href="{{{config.wwwroot}}}/local/seodashboard/secondcategory.php?status={{seoid}}&id={{currenturlid}}" onClick="javascript:return confirm('are you sure status update ?');" class='edit seostatus'><i class="fas {{#status}} 
                  fa-eye-slash 
                  {{/status}}{{^status}} fa-eye {{/status}}  "></i></a></li>{{/seoid}} 
               <li><a href="{{{config.wwwroot}}}/local/seodashboard/secondcategory.php?delete={{seoid}}&id={{currenturlid}}" class='edit seodelete' onClick="javascript:return confirm('are you sure you want to delete ?');"><i class="fas fa-trash-alt" ></i></a></li>
            </ul>
         </td>
      </tr>
  {{/filterdata}}
   </tbody>
   <tfoot>
      <tr>
         <td>Title</td>
         <td>URl</td>
         <th>Keywords</th>
            <th>Action</th>
      </tr>
   </tfoot>
</table>



