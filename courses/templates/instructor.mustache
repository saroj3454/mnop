<section  class="pt-5 pb-5 intructor" >
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="teaching-box">
          <div class="heading">
            <h2>MY COURSES</h2>
          </div> 
        </div>
      </div>
    </div>
    <div class="row" >
      <div class="col-md-12"> 
       <div class="row owl-carousel carousel-main" data-wrap="false" id="hover_zindex">
          {{#noofcourses}}
          <div class="col-md-12 mb-5">
            <a href="{{coursedetails}}">
                <div class="course_section tooltip_hover">
                  {{#courseimage}}<img src="{{courseimage}}" class="" width="250">{{/courseimage}}
                  <div class="tooltip_content tooltip_mobile">
                    <a class="vrz_heading">{{{fullname}}}</a><br>
                    <div class="best">
                      <div class="best_text"> Bestseller</div>
                      <span class="vritle">Updated<span class="vritle_xs"> {{update_date}}</span></span>

                    </div>
                    <div class="vrzite-text"><span> {{level}} </span></div>
                    <div class="vrt_content">{{{summary}}}</div>
                    {{! <div class="">
                      <ul class="vrt block-list">
                        <li><div class="vrt list-item"><span class="udlite-block-list-item-content">Master Machine Learning on Python &amp; R</span></div></li>
                        <li><div class="vrt list-item"><span class="udlite-block-list-item-content">Have a great intuition of many Machine Learning models</span></div></li>
                        <li><div class="vrt list-item"><span class="vrtitem-content">Make accurate predictions</span></div></li>
                      </ul>
                    </div> }}
                    <div class="course-details" align="center"><a href="{{enroll}}" class=" btn btn-rose black-hover">Enroll Now</a></div>
                  </div>

                  <h5 class="h5 text-left my-3 coursename">{{{fullname}}}</h5>
                  {{#teachers}}
                  <label class="teacher"><a href="{{{user_url1}}}" target="_blank">{{teacher_name}}</a></label>
                  {{/teachers}}
                  <!-- <p class="course_description">{{{summary}}}</p> -->
                  <!-- <p class="course_highlights"></p> -->
                  <p class="total_enrolled"> <strong> Enrolled Students : </strong> {{enrollStudent}}</p>
                  <div class=" right">
                    <span>Price:&nbsp;{{course_price}}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <a href="{{enroll}}" class=" btn btn-rose black-hover">Enroll Now</a>
                  </div>
                </div>
            </a>
          </div>
          {{/noofcourses}}
         
        </div>
      </div>
    </div>
</div>
</section>

<section  class="bg-gray pt-5 pb-5 intructor_blog">
  <div class="container">
    <div class="row owl-carousel carousel-main" data-wrap="false" id="hover_zindex">
      <div class="col-md-12">
        <div class="teaching-box">
          <div class="heading">
            <h2>MY BLOGS</h2><br>
          </div> 
        </div>
      </div>
    </div>
    <div  class="row owl-carousel carousel-main" data-wrap="false">
       {{#blogs}}
      <div class="col-md-12">
      <div class="course_section">
        <a href="{{blogdetails}}" title="{{{blog_title}}}">
          <img src="{{blog_image}}" class="courseimage" width="250">
          <div class="category_caption p-2">
            <h6 class="h5 text-left my-3 coursename">{{{blog_title}}}</h6>
            <p class="course_highlights">{{{summary}}}</p>
            <p class="course_description">{{author_name}}</p>
            <p class="course_description">{{categoryname}}</p>
          </div>
        </a>
      </div>
    </div>
      {{/blogs}}
    </div>
  </div>
</section>
