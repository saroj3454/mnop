<!-- Course details page -->
{{>local_courses/courseheader}}

<section class="my-5 pt-5">
  <div class="container">
    <div class="row">
     <div class="col-md-2">
       <div class="user-profile">
         <img src="https://www.vrtuz.com//local/courses/images/head.png" class="img-fluid">
       </div>
     </div>
     <div class="col-md-5">
          <div class="col-md-12">
            <div class="user-info">
              <br>
              {{#totals}}
               <h2>{{tutor_name}}</h2>
               {{/totals}}
               <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit </p>
               
             </div>
          </div>
            <div class="container">
                 <div class="row">
                   <div class="col-md-6">
                    <div class="user-info">
                        {{#totals}}
                       <p>No. of enrolled student <span>{{total_learner}}</span></p>
                        {{/totals}}
                     </div>
                  </div>
                  <div class="col-md-6">
                    <div class="user-info">
                       {{#totals}}
                       <p>No. of enrolled courses <span>{{total_course}}</span></p>
                        {{/totals}}
                     </div>
                  </div>
                 </div>
            </div>
     </div>
   </div>
 </div>
</section>
